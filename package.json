{
  "name": "portfolio",
  "private": true,
  "description": "A portfolio website of Sayan Mondal",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-sharp": "^2.6.40",
    "gatsby-plugin-styled-components": "^3.3.11",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.17",
    "modern-css-reset": "^1.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.2.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/S-ayanide/portfolio"
